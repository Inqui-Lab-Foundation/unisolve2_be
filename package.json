    "seed": "node src/database/seeders/seed.js",
    "migrate:sql": "node src/database/migrations/migrateSql.js",
    "seed:sql": "node src/database/seeders/seedSql.js",
    "scaffold:entity": "node -e 'require(\"./src/scaffolders/generate.js\").g()'"
